<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="372.0" prefWidth="106.0" type="VBox" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="105.0" fitWidth="105.0" onMouseClicked="#loadWelcome" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/img/logo.png" />
         </image>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </ImageView>
      <JFXButton fx:id="btnMembers" alignment="CENTER" buttonType="RAISED" onAction="#loadMembers" prefHeight="40.0" prefWidth="162.0" style="-fx-background-color: #336566;" text="Leden" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
         </VBox.margin>
      </JFXButton>
      <JFXButton fx:id="btnActivities" alignment="CENTER" buttonType="RAISED" onAction="#loadActivities" prefHeight="40.0" prefWidth="158.0" style="-fx-background-color: #336566;" text="Activiteiten" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </JFXButton>
      <JFXButton fx:id="btnCourseMaterial" alignment="CENTER" buttonType="RAISED" onAction="#loadCourseMaterial" prefHeight="40.0" prefWidth="142.0" style="-fx-background-color: #336566;" text="Lesmateriaal" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </JFXButton>
      <JFXButton fx:id="btnOverviews" alignment="CENTER" buttonType="RAISED" onAction="#loadOverviews" prefHeight="40.0" prefWidth="140.0" style="-fx-background-color: #336566;" text="Overzichten" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </JFXButton>
      <JFXButton fx:id="btnLeaderBoard" alignment="CENTER" buttonType="RAISED" onAction="#loadLeaderBoard" prefHeight="40.0" prefWidth="140.0" style="-fx-background-color: #336566;" text="Scorebord" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
         <VBox.margin>
            <Insets bottom="15.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </JFXButton>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</fx:root>
